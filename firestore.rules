rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /all_cocktail_recipe/{document=**} {
      allow read, create;
      allow delete: if false;
      allow update: if false;
    }
    match /all_ingredient/{document=**} {
      allow read, create;
      allow delete: if false;
      allow update: if false;
    }
    match /all_glass_type/{document=**} {
      allow read, create;
      allow delete: if false;
      allow update: if false;
    }
    match /members_creations/{document=**} {
      allow read;
      allow write: if request.auth != null;
    }
    match /members/{document=**} {
      allow read;
      allow write: if request.auth != null;
    }
  }
}